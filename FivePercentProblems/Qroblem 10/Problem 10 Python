notdone = True
number = 1
primesum = 0

while notdone:
    number += 1
    for x in range(1, number):
        if number % x == 0:
            if number > 2000000:
                notdone = False
                break
            primesum += number
        else:
            if x == number - 1:
                break
            continue

print(primesum)
