# Again not trying to be efficient
notdone = True
primes = []
number = 2
primetofind = 10001

while notdone:
    for x in range(2, number):
        if number % x == 0:
            break
        else:
            if x == number - 1:
                primes.append(number)
                break
            continue
    number += 1 
    if len(primes) == primetofind:
        notdone = False

print(primes[primetofind - 1])
